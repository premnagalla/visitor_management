<%- model_class = TenantLocker -%>
<div class="page-header">
  <h1><%=t '.title', :default => model_class.model_name.human.pluralize.titleize %></h1>
</div>
<table class="table table-striped">
  <thead>
    <tr>
      <th><%= model_class.human_attribute_name(:id) %></th>
      <th><%= model_class.human_attribute_name(:user_id) %></th>
      <th><%= model_class.human_attribute_name(:locker_id) %></th>
      <th><%= model_class.human_attribute_name(:start_date) %></th>
      <th><%= model_class.human_attribute_name(:end_date) %></th>
      <th><%= model_class.human_attribute_name(:created_at) %></th>
      <th><%=t '.actions', :default => t("helpers.actions") %></th>
    </tr>
  </thead>
  <tbody>
    <% @tenant_lockers.each do |tenant_locker| %>
      <tr>
        <td><%= link_to tenant_locker.id, tenant_locker_path(tenant_locker) %></td>
        <td><%= tenant_locker.user_id %></td>
        <td><%= tenant_locker.locker_id %></td>
        <td><%= tenant_locker.start_date %></td>
        <td><%= tenant_locker.end_date %></td>
        <td><%=l tenant_locker.created_at %></td>
        <td>
          <%= link_to t('.show', :default => t("helpers.links.show")),
                      tenant_locker_path(tenant_locker), :class => 'btn btn-default btn-xs' %>
          <%= link_to t('.edit', :default => t("helpers.links.edit")),
                      edit_tenant_locker_path(tenant_locker), :class => 'btn btn-default btn-xs' %>
          <%= link_to t('.destroy', :default => t("helpers.links.destroy")),
                      tenant_locker_path(tenant_locker),
                      :method => :delete,
                      :data => { :confirm => t('.confirm', :default => t("helpers.links.confirm", :default => 'Are you sure?')) },
                      :class => 'btn btn-xs btn-danger' %>
        </td>
      </tr>
    <% end %>
  </tbody>
</table>

<%= link_to t('.new', :default => t("helpers.links.new")),
            new_tenant_locker_path,
            :class => 'btn btn-primary' %>
